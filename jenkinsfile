#!/usr/bin/env groovy

@Library('shared-library') _
pipeline {
    agent none
    stages {
        stage('build') {
            steps {
                script {
                    buildFunc()
                }
            }
        }
        stage('test') {
            steps {
                script {
                    testFunc()
                }
            }
        }
        stage('deploy') {
            steps {
                script {
                    deployFunc()
                }
            }
        }
    }
}

